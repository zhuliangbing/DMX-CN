    <name>DMX Test</name>
    <groupId>systems.dmx</groupId>
    <artifactId>dmx-test</artifactId>
    <version>5.0-beta-4-SNAPSHOT</version>
    <packaging>jar</packaging>

    <parent>
        <groupId>systems.dmx</groupId>
        <artifactId>dmx</artifactId>
        <version>5.0-beta-4-SNAPSHOT</version>
        <relativePath>../../pom.xml</relativePath>
